{
  "name": "papyr-core",
  "version": "0.1.1",
  "description": "Core package for Papyr - a markdown knowledge management system",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "runtime": [
        "dist/runtime.d.ts"
      ],
      "graph": [
        "dist/graph.d.ts"
      ],
      "graphAnalysis": [
        "dist/graphAnalysis.d.ts"
      ],
      "linkResolver": [
        "dist/linkResolver.d.ts"
      ],
      "folderHierarchy": [
        "dist/folderHierarchy.d.ts"
      ],
      "search": [
        "dist/search.d.ts"
      ],
      "jsonExporter": [
        "dist/jsonExporter.d.ts"
      ],
      "analytics": [
        "dist/analytics.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./runtime": {
      "import": "./dist/runtime.js",
      "types": "./dist/runtime.d.ts"
    },
    "./graph": {
      "import": "./dist/graph.js",
      "types": "./dist/graph.d.ts"
    },
    "./graphAnalysis": {
      "import": "./dist/graphAnalysis.js",
      "types": "./dist/graphAnalysis.d.ts"
    },
    "./linkResolver": {
      "import": "./dist/linkResolver.js",
      "types": "./dist/linkResolver.d.ts"
    },
    "./folderHierarchy": {
      "import": "./dist/folderHierarchy.js",
      "types": "./dist/folderHierarchy.d.ts"
    },
    "./search": {
      "import": "./dist/search.js",
      "types": "./dist/search.d.ts"
    },
    "./jsonExporter": {
      "import": "./dist/jsonExporter.js",
      "types": "./dist/jsonExporter.d.ts"
    },
    "./analytics": {
      "import": "./dist/analytics.js",
      "types": "./dist/analytics.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest --run",
    "test:coverage": "vitest --coverage --run",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "markdown",
    "knowledge-management",
    "graph",
    "analytics",
    "search",
    "wiki-links"
  ],
  "author": "Ray Kong",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ray-kong/papyr-core.git"
  },
  "bugs": {
    "url": "https://github.com/Ray-kong/papyr-core/issues"
  },
  "homepage": "https://github.com/Ray-kong/papyr-core#readme",
  "dependencies": {
    "flexsearch": "^0.7.31",
    "gray-matter": "^4.0.3",
    "rehype-highlight": "^7.0.0",
    "rehype-stringify": "^10.0.0",
    "remark": "^15.0.0",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.0.0",
    "remark-wiki-link": "^2.0.1",
    "slugify": "^1.6.6",
    "unified": "^11.0.0",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^1.6.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.0.0",
    "vitest": "^1.6.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
